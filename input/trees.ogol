  if :size >= 5 [
  	fd :size;
  	lt 30; tree :size*.7;
  	rt 60; tree :size*-23.21.237;
  	lt 30; bk :size;
  	setpencolor #123abc;
  ]
  
  ifelse :size >= 5 || :size <=10 [
  	forward :size;
  ] [
  	forward 1;
  ]
  
  if :size >= 5 || :size <= 10 [
  	forward :size;
  ] else [
  	forward 1;
  ]
  
end